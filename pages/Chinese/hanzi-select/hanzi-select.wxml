<!--pages/Chinese/hanzi-select/hanzi-select.wxml-->
<view class="container">
    <navigation-bar
        class="navigation-bar"
        back="{{true}}"
        background="#c8e8e8"
        color="#333"
    ></navigation-bar>
    
    <image class="title-pic" src="{{titleUrl}}"></image>
    <image class="easy-pic" src="{{easyUrl}}" bindtap="onEasy"></image>
    <image class="medium-pic" src="{{mediumUrl}}" bindtap="onMedium"></image>
    <image class="hard-pic" src="{{hardUrl}}" bindtap="onHard"></image>
</view>